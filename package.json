{
  "name": "client-web-api-sandbox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --cache \"src/**/*\"",
    "lint:fix": "eslint --cache \"src/**/*\" --fix",
    "format": "prettier --cache --check \"./src\"",
    "format:fix": "prettier --cache --write \"./src\"",
    "markup": "markuplint \"src/**/*.{jsx,tsx,html}\"",
    "markup:fix": "markuplint \"src/**/*.{jsx,tsx,html}\" --fix",
    "check": "npm run markup && npm run lint && npm run format",
    "check:fix": "npm run markup:fix && npm run lint:fix && npm run format:fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "prepare": "husky"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.4.5",
    "@mui/x-tree-view": "^8.19.0",
    "@okathira/ghostpdl-wasm": "^1.1.0",
    "@xmldom/xmldom": "^0.8.11",
    "fast-xml-parser": "^5.3.6",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.1.0",
    "jotai": "^2.15.0",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.4.1",
    "react-xml-viewer": "^3.0.1",
    "three": "^0.174.0",
    "tone": "^15.0.4",
    "xpath": "^0.0.34"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.25.0",
    "@markuplint/jsx-parser": "^4.7.19",
    "@markuplint/react-spec": "^4.5.14",
    "@testing-library/react": "^16.3.0",
    "@types/dom-mediacapture-transform": "^0.1.11",
    "@types/emscripten": "^1.41.4",
    "@types/eslint__js": "^8.42.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/papaparse": "^5.5.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.174.0",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-html": "^8.0.0",
    "eslint-plugin-import-x": "^4.11.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.1",
    "markuplint": "^4.14.1",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "ts-jest-mock-import-meta": "^1.3.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^7.2.4"
  },
  "lint-staged": {
    "*.{jsx,tsx,html}": [
      "markuplint"
    ],
    "src/**/*": [
      "eslint --cache",
      "prettier --check"
    ]
  }
}
