<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<DatDefine VR="1.0">
<!--寄附金控除に関する証明書 TEG830-->
	<report reportVR="1.0" reportId="TEG830" columnNum="25">
		<!--寄附者別XML作成用グルーピングコード-->
		<item type="Normal" column="2">
			<valid id="charSet" value="alphaNumeric"/>
			<valid id="length" max="4" messageId="HCD6033E"/>
		</item>
		<!--寄附者の氏名-->
		<item type="Normal" column="3">
			<valid id="exist"/>
			<valid id="length" max="100" messageId="HCD6039E"/>
			<valid id="etxString" messageId="HCD6023E"/>
		</item>
		<!--寄附者の住所-->
		<item type="Normal" column="4">
			<valid id="length" max="100" messageId="HCD6039E"/>
			<valid id="etxString" messageId="HCD6023E"/>
		</item>
		<!--寄附者ＩＤ-->
		<item type="Normal" column="5">
			<valid id="charSet" value="latinString"/>
			<valid id="length" max="20" messageId="HCD6050E"/>
		</item>
		<!--証明書発行者名-->
		<item type="Normal" column="6">
			<valid id="exist"/>
			<valid id="length" max="20" messageId="HCD6050E"/>
			<valid id="etxString" messageId="HCD6023E"/>
		</item>
		<!--証明書発行者法人番号-->
		<item type="Normal" column="7">
			<valid id="charSet" value="numeral"/>
			<valid id="length" value="13" messageId="HCD6048E"/>
			<valid id="format" value="checkDigits"/>
		</item>
		<!--証明書発行年月日-->
		<item type="Normal" column="8">
			<valid id="existDate2" value="date">
				<outer id="exist" column="8"/>
				<outer id="charSet" column="8" value="numeral"/>
				<outer id="length" column="8" value="1" messageId="HCD6159E"/>
				<outer id="contain" column="8" value="5" messageId="HCD6158E"/>
				<outer id="exist" column="9"/>
				<outer id="charSet" column="9" value="numeral"/>
				<outer id="length" column="9" max="2" messageId="HCD6042E"/>
				<outer id="range" column="9" min="1" max="99" messageId="HCD6085E"/>
				<outer id="exist" column="10"/>
				<outer id="charSet" column="10" value="numeral"/>
				<outer id="length" column="10" max="2" messageId="HCD6042E"/>
				<outer id="range" column="10" min="1" max="12" messageId="HCD6043E"/>
				<outer id="exist" column="11"/>
				<outer id="charSet" column="11" value="numeral"/>
				<outer id="length" column="11" max="2" messageId="HCD6042E"/>
				<outer id="range" column="11" min="1" max="31" messageId="HCD6044E"/>
			</valid>
		</item>
		<!--証明書発行年月日　元号-->
		<item type="Normal" column="8">
			<valid id="exist"/>
			<valid id="charSet" value="numeral"/>
			<valid id="length" value="1" messageId="HCD6159E"/>
			<valid id="contain" value="5" messageId="HCD6158E"/>
		</item>
		<!--証明書発行年月日　年-->
		<item type="Normal" column="9">
			<valid id="exist"/>
			<valid id="charSet" value="numeral"/>
			<valid id="length" max="2" messageId="HCD6042E"/>
			<valid id="range" min="1" max="99" messageId="HCD6085E"/>
		</item>
		<!--証明書発行年月日　月-->
		<item type="Normal" column="10">
			<valid id="exist"/>
			<valid id="charSet" value="numeral"/>
			<valid id="length" max="2" messageId="HCD6042E"/>
			<valid id="range" min="1" max="12" messageId="HCD6043E"/>
		</item>
		<!--証明書発行年月日　日-->
		<item type="Normal" column="11">
			<valid id="exist"/>
			<valid id="charSet" value="numeral"/>
			<valid id="length" max="2" messageId="HCD6042E"/>
			<valid id="range" min="1" max="31" messageId="HCD6044E"/>
		</item>
		<!--証明年分　元号-->
		<item type="Normal" column="12">
			<valid id="exist"/>
			<valid id="charSet" value="numeral"/>
			<valid id="length" value="1" messageId="HCD6159E"/>
			<valid id="contain" value="5" messageId="HCD6158E"/>
		</item>
		<!--証明年分　年-->
		<item type="Normal" column="13">
			<valid id="exist"/>
			<valid id="charSet" value="numeral"/>
			<valid id="length" max="2" messageId="HCD6042E"/>
			<valid id="range" min="1" max="99" messageId="HCD6085E"/>
		</item>
		<!--寄附の種類-->
		<item type="Normal" column="14">
			<valid id="exist"/>
			<valid id="contain" value="1 2 3 4 5 6 7 8" messageId="HCD6157E"/>
		</item>
		<!--適用条文（その他）-->
		<item type="Normal" column="15">
			<valid id="exist" messageId="HCD6156E">
				<outer id="exist" column="14"/>
				<outer id="contain" value="8" column="14" messageId="HCD6157E"/>
			</valid>
			<valid id="length" max="90" messageId="HCD6155E"/>
			<valid id="etxString" messageId="HCD6023E"/>
		</item>
		<!--年間寄附金額-->
		<item type="Normal" column="16">
			<valid id="exist"/>
			<valid id="charSet" value="nonNegativeInteger"/>
			<valid id="range" min="0" max="9999999999999" messageId="HCD6161E"/>
		</item>
		<!--寄附番号-->
		<item type="Normal" column="17">
			<valid id="charSet" value="latinString"/>
			<valid id="length" max="50" messageId="HCD6059E"/>
		</item>
		<!--寄附年月日-->
		<item type="Normal" column="18">
			<valid id="existDate" value="date">
				<outer id="exist" column="18"/>
				<outer id="charSet" column="18" value="numeral"/>
				<outer id="length" column="18" value="4" messageId="HCD6040E"/>
				<outer id="range" column="18" min="1" max="9999" messageId="HCD6067E"/>
				<outer id="exist" column="19"/>
				<outer id="charSet" column="19" value="numeral"/>
				<outer id="length" column="19" max="2" messageId="HCD6042E"/>
				<outer id="range" column="19" min="1" max="12" messageId="HCD6043E"/>
				<outer id="exist" column="20"/>
				<outer id="charSet" column="20" value="numeral"/>
				<outer id="length" column="20" max="2" messageId="HCD6042E"/>
				<outer id="range" column="20" min="1" max="31" messageId="HCD6044E"/>
			</valid>
		</item>
		<!--寄附年月日　年-->
		<item type="Normal" column="18">
			<valid id="exist"/>
			<valid id="charSet" value="numeral"/>
			<valid id="length" value="4" messageId="HCD6040E"/>
			<valid id="range" min="1" max="9999" messageId="HCD6067E"/>
		</item>
		<!--寄附年月日　月-->
		<item type="Normal" column="19">
			<valid id="exist"/>
			<valid id="charSet" value="numeral"/>
			<valid id="length" max="2" messageId="HCD6042E"/>
			<valid id="range" min="1" max="12" messageId="HCD6043E"/>
		</item>
		<!--寄附年月日　日-->
		<item type="Normal" column="20">
			<valid id="exist"/>
			<valid id="charSet" value="numeral"/>
			<valid id="length" max="2" messageId="HCD6042E"/>
			<valid id="range" min="1" max="31" messageId="HCD6044E"/>
		</item>
		<!--寄附先に関する事項　寄附を受けた団体名-->
		<item type="Normal" column="21">
			<valid id="exist"/>
			<valid id="length" max="20" messageId="HCD6050E"/>
			<valid id="etxString" messageId="HCD6023E"/>
		</item>
		<!--寄附先に関する事項　寄附を受けた団体の法人番号-->
		<item type="Normal" column="22">
			<valid id="charSet" value="numeral"/>
			<valid id="length" value="13" messageId="HCD6048E"/>
			<valid id="format" value="checkDigits"/>
		</item>
		<!--寄附金額-->
		<item type="Normal" column="23">
			<valid id="exist"/>
			<valid id="charSet" value="nonNegativeInteger"/>
			<valid id="range" min="0" max="9999999999999" messageId="HCD6161E"/>
		</item>
		<!--備考-->
		<item type="Normal" column="24">
			<valid id="length" max="50" messageId="HCD6059E"/>
			<valid id="etxString" messageId="HCD6023E"/>
		</item>
		<!--キャンセル情報-->
		<item type="Normal" column="25">
			<valid id="charSet" value="numeral"/>
			<valid id="length" value="1" messageId="HCD6159E"/>
			<valid id="contain" value="0 1" messageId="HCD6084E"/>
		</item>
	</report>
</DatDefine>
